{
  "name": "myexomeanalyzer",
  "version": "1.0.0",
  "description": "Clinical genomics analysis platform with TileDB optimization, clinical safety features, and AI-powered variant interpretation via MCP",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "analyze": "node dist/cli/index.js",
    "mcp-server": "node dist/mcp-server/server.js",
    "test": "node --test",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "setup": "node dist/cli/index.js setup",
    "example": "./scripts/example-workflow.sh"
  },
  "bin": {
    "myexome": "dist/cli/index.js"
  },
  "keywords": [
    "vcf",
    "exome",
    "genomics",
    "mcp",
    "llm",
    "bioinformatics",
    "clinical-genomics",
    "tiledb",
    "variant-analysis",
    "claude",
    "ai-genomics",
    "clinical-safety",
    "cardiac-genetics",
    "precision-medicine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
